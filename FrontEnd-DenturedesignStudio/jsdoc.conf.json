{
  "tags": {
    "allowUnknownTags": true
  },

  "source": {
    "include": ["src"], 
    "includePattern": "\\.jsx?$",
    "excludePattern": "(node_modules/|test|spec)\\.jsx?$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs", 
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/clean-jsdoc-theme"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
    
  }
}
